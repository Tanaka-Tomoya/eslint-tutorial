{
    "rules": {
        "no-console": "off",
        "no-extra-semi": "warn",
        "no-undef": "warn",
        "quotes": ["warn", "single"],
        "space-before-blocks": ["warn", { "functions": "always"}]
    },
    "globals": {
    },
    "parserOptions": {
        "ecmaVersion": 2015
    },
    "env": {
        "browser": true,
        "jquery": true
    },
    "extends": "eslint:recommended"
}